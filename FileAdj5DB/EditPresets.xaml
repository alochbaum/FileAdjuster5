<Window x:Name="wndEditPresets" x:Class="FileAdj5DB.EditPresets"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileAdj5DB"
        mc:Ignorable="d"
        Title="EditPresets" Height="450" Width="600" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="3*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60*"/>
            <ColumnDefinition Width="660*"/>
            <ColumnDefinition Width="60*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnAdd2Target" Grid.Column="1" Grid.Row="1" Click="BtnAdd2Target_Click">Add Selected Preset Group to Target Database</Button>
        <DataGrid x:Name="DisplayGrid" Grid.Column="1" HorizontalAlignment="Stretch" Margin="2" Grid.Row="3" CellEditEnding="DisplayGrid_CellEditEnding" />

    </Grid>
</Window>
